{
  "name": "thinky-export-schema",
  "version": "1.0.0",
  "description": "Description goes here",
  "main": "dist/index.js",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contra/thinky-export-schema.git"
  },
  "author": "Contra <yo@contra.io> (http://contra.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/contra/thinky-export-schema/issues"
  },
  "homepage": "https://github.com/contra/thinky-export-schema#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "preversion": "npm run clean && npm run build",
    "postversion": "npm run changelog",
    "build": "babel src --out-dir dist",
    "clean": "rimraf dist",
    "lint": "eslint src",
    "changelog": "github-changes -o contra -r thinky-export-schema -b master -f ./CHANGELOG.md --order-semver --use-commit-body",
    "test": "npm run-script lint && mocha --compilers js:babel-register --recursive --reporter spec"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "eslint": "^1.10.3",
    "eslint-config-rackt": "^1.1.1",
    "github-changes": "^1.0.1",
    "mocha": "^2.3.4",
    "rimraf": "^2.5.0",
    "should": "^8.0.0",
    "thinky": "^2.2.4"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime",
      "add-module-exports"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "rackt",
    "env": {
      "node": true,
      "es6": true
    },
    "ecmaFeatures": {
      "modules": true
    }
  },
  "dependencies": {
    "lodash.mapvalues": "^4.0.1"
  }
}
